<launch>  
	<node pkg="move_base" type="move_base" name="move_base" output="screen">
	      <param name="controller_frequency" value="10.0"/>
	    <rosparam file="$(find pedsim_simulator)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
	    <rosparam file="$(find pedsim_simulator)/params/global_costmap_params.yaml" command="load" />
	    <rosparam file="$(find pedsim_simulator)/params/trajectory_planner.yaml" command="load" />
	  </node>
</launch>
